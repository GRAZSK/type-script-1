
type Facultative = "Programming" | "Art" | "Sports" | "Music" | "Robotics"
type Student = {
    name: string
    age: number
    facultative: Facultative
}

const student1: Student = {
    name: "Ivan",
    age: 17,
    facultative: "Programming"
}

const student2: Student = {
    name: "Anna",
    age: 16,
    facultative: "Art"
}

console.log(student1) // { name: 'Ivan', age: 17, facultative: 'Programming' }
console.log(student2) // { name: 'Anna', age: 16, facultative: 'Art' }
